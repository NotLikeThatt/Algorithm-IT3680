package day_2;

import java.io.FileInputStream;
import java.io.FileWriter;
import java.io.IOException;
import java.io.PrintWriter;
import java.util.Scanner;

public class Add {
	public static final String PATH_INPUT_FILE = "D:\\Add\\Add09.inp";
	public static final String PATH_OUPUT_FILE = "D:\\Add\\Add09.out";

	public static void main(String[] args) throws IOException {
		System.setIn(new FileInputStream(PATH_INPUT_FILE));
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		int b = scanner.nextInt();
		try (PrintWriter writer = new PrintWriter(new FileWriter(PATH_OUPUT_FILE, false), true)) {
			writer.print(a + b);
		}
		System.out.println(a + b);
		scanner.close();
	}
}
