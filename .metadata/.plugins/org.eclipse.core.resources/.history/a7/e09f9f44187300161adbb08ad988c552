package day_2;

import java.io.FileWriter;
import java.io.IOException;
import java.io.PrintWriter;
import java.util.Random;
import java.util.Scanner;

public class InitInput {
	public static final String PATH_FILE = "D:\\input_sequence.txt";

	public static void main(String[] args) {
		Scanner inp = new Scanner(System.in);
		int numberOfInteger = inp.nextInt();
		try (PrintWriter writer = new PrintWriter(new FileWriter(PATH_FILE, true), true)) {
			writer.println(numberOfInteger);
			Random random = new Random();
			String token = "";
			for (int i = 0; i < numberOfInteger; i++) {
				token += random.nextInt();
				if (i != numberOfInteger - 1) {
					token += ", ";
				}
			}
		} catch (IOException e) {
			System.out.println(e.getMessage());
		}
		inp.close();
	}
}
