package day_2;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class Main {
	public static void main(String[] args) {
		int[] inputData;
		try (BufferedReader inp = new BufferedReader(new InputStreamReader(System.in))) {
			int numberOfInteger = inp.read() - '0';
			System.out.println(numberOfInteger);
			inputData = new int[numberOfInteger];

			for (int i = 0; i < numberOfInteger; i++) {
				inputData[i] = inp.read() - '0';
			}
			inp.close();
		} catch (IOException e) {
			System.out.println(e.getMessage());
			System.exit(1);
		}
	}

	public static long dynamicProgrammingAlgo(int[] a) {
		int n = a.length;
		int[] s = new int[n];
		s[0] = a[0];
		long max = s[0];
		for (int i = 1; i < n; i++) {
			if (s[i - 1] > 0) {
				s[i] = s[i - 1] + a[i];
			} else {
				s[i] = a[i];
			}
			max = max > s[i] ? max : s[i];
		}
		return max;
	}
}
