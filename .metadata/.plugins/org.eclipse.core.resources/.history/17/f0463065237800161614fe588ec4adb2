package sorts;

public class SelectionSort {

	public static void main(String[] args) {
	}

	// Sort by descending
	public static void sort(int[] a) {
		for (int i = 0; i < a.length; i++) {
			int max = a[i]; // Lưu phần tử lớn nhất
			int index = i; // lưu vị trí chứa phần tử lớn nhất
		}
	}
}
